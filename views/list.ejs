
    <%- include('header'); -%> 
    <div class="box" id="heading">
        <h1><%= listTitle %> </h1>
    </div>
    
    <div class="box">
        <ul>
            <% for (item of newListItems){%>
                <div class="item">
                    <input type="checkbox">
                    <p><%= item %></p>
                </div>
                
            <% } %>
        </ul>
        <form action="/" method="post">
            <input type="text" placeholder="Enter item to be added" name="newValue" autocomplete="off">
            <button type="submit" name="list" value=<%= listTitle %>>+</button>
        </form>
    </div>
    <%- include ('footer'); -%> 